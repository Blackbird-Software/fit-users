{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "aws/aws-sdk-php": "^3.112",
        "bref/bref": "^0.5.7",
        "friendsofsymfony/rest-bundle": "^2.5",
        "jms/serializer-bundle": "^3.4",
        "symfony/console": "4.3.*",
        "symfony/dotenv": "4.3.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "4.3.*",
        "symfony/monolog-bundle": "^3.4",
        "symfony/security-bundle": "4.3.*",
        "symfony/security-core": "4.3.*",
        "symfony/security-guard": "4.3.*",
        "symfony/security-http": "4.3.*",
        "symfony/twig-bundle": "4.3.*",
        "symfony/yaml": "4.3.*"
    },
    "require-dev": {
        "nelmio/api-doc-bundle": "^3.4",
        "object-calisthenics/phpcs-calisthenics-rules": "^3.5",
        "phploc/phploc": "^5.0",
        "phpstan/phpstan": "^0.11.16",
        "phpstan/phpstan-doctrine": "^0.11.6",
        "phpstan/phpstan-strict-rules": "^0.11.1",
        "sensiolabs/security-checker": "^6.0",
        "sylius-labs/coding-standard": "^3.1",
        "symfony/maker-bundle": "^1.13",
        "symfony/profiler-pack": "^1.0",
        "symfony/var-dumper": "4.3.*",
        "symfony/web-server-bundle": "4.3.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "check": [
            "composer validate --strict",
            "@analyse"
        ],
        "fix": [
            "vendor/bin/ecs check --ansi --no-progress-bar bin public src --config easy-coding-standards.yml --fix"
        ],
        "analyse": [
            "vendor/bin/ecs check --ansi --no-progress-bar --config easy-coding-standards.yml bin public src",
            "vendor/bin/phpstan analyse --ansi --no-progress -c phpstan.neon -l 7 bin public src"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.3.*"
        }
    }
}
